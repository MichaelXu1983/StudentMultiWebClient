<block wx:if="{{$taroCompReady}}">
    <view class="{{styles['help']}}">
        <view class="{{styles['help-link__item']}}">
            <view class="{{styles['help-link__icon']}}">
                <image src="{{help_guide}}" class="{{styles['help-link__icon-help']}}"></image>
            </view>
            <view class="{{styles['help-link__ask']}}">
                <navigator url="/pages/webview/attention" class="{{styles['index__Navigator']}}">
                    <view class="{{styles['help-link__title']}}"><text class="{{styles['help-link__title-text']}}">上课须知 ？</text>
                    </view>
                </navigator>
                <navigator url="/pages/webview/guide" class="{{styles['index__Navigator']}}">
                    <view class="{{styles['help-link__title help-link__title-noborder']}}"><text class="{{styles['help-link__title-text']}}">上课指南 ？</text>
                    </view>
                </navigator>
            </view>
        </view>
        <view class="{{styles['help-link__separator']}}"></view>
        <view class="{{styles['help-link__item']}}">
            <view class="{{styles['help-link__icon']}}">
                <image src="{{help_update}}" class="{{styles['help-link__icon-help']}}"></image>
            </view>
            <view class="{{styles['help-link__ask']}}">
                <navigator url="/pages/webview/update_time" class="{{styles['index__Navigator']}}">
                    <view class="{{styles['help-link__title']}}"><text class="{{styles['help-link__title-text']}}">我们多长时间更新一次课程 ？</text>
                    </view>
                </navigator>
                <navigator url="/pages/webview/update_notice" class="{{styles['index__Navigator']}}">
                    <view class="{{styles['help-link__title']}}"><text class="{{styles['help-link__title-text']}}">如何收到课程更新通知 ？</text>
                    </view>
                </navigator>
            </view>
        </view>
    </view>
</block>