<block wx:if="{{$taroCompReady}}">
    <view class="{{styles['users']}}">
        <image src="{{usersBg}}" class="{{styles['users__bg']}}"></image>
        <view class="{{styles['users__avatarUrl']}}">
            <image src="{{avatar}}" class="{{styles['users__avatarUrl-img']}}"></image>
        </view>
        <view class="{{styles['users-card']}}">
            <view class="{{styles['users-card__username']}}"><text class="{{styles['users-card__text-title']}}">小朋友</text>
            </view>
        </view>
        <view class="{{styles['users-link']}}">
            <navigator url="/pages/index/message_list" class="{{styles['index__Navigator']}}">
                <view class="{{styles['users-link__item']}}">
                    <view class="{{styles['users-link__icon']}}">
                        <image src="{{message}}" class="{{styles['users-link__icon-help']}}"></image>
                    </view>
                    <view class="{{styles['users-link__title']}}"><text class="{{styles['users-link__title-text']}}">消息中心</text>
                    </view>
                    <view class="{{styles['users-link__subtitle']}}"><text class="{{styles['users-link__subtitle-text']}}"></text>
                    </view>
                    <view class="{{styles['users-link__arrow']}}">
                        <image src="{{arrowRight}}" class="{{styles['users-link__arrow--active']}}"></image>
                    </view>
                </view>
            </navigator>
            <navigator url="/pages/index/notice_list" class="{{styles['index__Navigator']}}">
                <view class="{{styles['users-link__item']}}">
                    <view class="{{styles['users-link__icon']}}">
                        <image src="{{notice}}" class="{{styles['users-link__icon-help']}}"></image>
                    </view>
                    <view class="{{styles['users-link__title']}}"><text class="{{styles['users-link__title-text']}}">公告中心</text>
                    </view>
                    <view class="{{styles['users-link__subtitle']}}"><text class="{{styles['users-link__subtitle-text']}}"></text>
                    </view>
                    <view class="{{styles['users-link__arrow']}}">
                        <image src="{{arrowRight}}" class="{{styles['users-link__arrow--active']}}"></image>
                    </view>
                </view>
            </navigator>
            <navigator url="/pages/webview/help" class="{{styles['index__Navigator']}}">
                <view class="{{styles['users-link__item']}}">
                    <view class="{{styles['users-link__icon']}}">
                        <image src="{{help}}" class="{{styles['users-link__icon-help']}}"></image>
                    </view>
                    <view class="{{styles['users-link__title']}}"><text class="{{styles['users-link__title-text']}}">帮助中心</text>
                    </view>
                    <view class="{{styles['users-link__arrow']}}">
                        <image src="{{arrowRight}}" class="{{styles['users-link__arrow--active']}}"></image>
                    </view>
                </view>
            </navigator>
            <navigator url="/pages/users/setting" class="{{styles['index__Navigator']}}">
                <view class="{{styles['users-link__item']}}">
                    <view class="{{styles['users-link__icon']}}">
                        <image src="{{setting}}" class="{{styles['users-link__icon-help']}}"></image>
                    </view>
                    <view class="{{styles['users-link__title']}}"><text class="{{styles['users-link__title-text']}}">设置</text>
                    </view>
                    <view class="{{styles['users-link__arrow']}}">
                        <image src="{{arrowRight}}" class="{{styles['users-link__arrow--active']}}"></image>
                    </view>
                </view>
            </navigator>
            <block>
                <block wx:if="{{env === 'weapp'}}">
                    <view class="{{styles['users-link__item']}}">
                        <view class="{{styles['users-link__icon']}}">
                            <image src="{{customer}}" class="{{styles['users-link__icon-help']}}"></image>
                        </view>
                        <view>
                            <view class="{{styles['users-link__title']}}"><text class="{{styles['users-link__title-text']}}">在线客服 </text>
                            </view>
                            <view class="{{styles['users-link__subtitle']}}">
                                <contact-button></contact-button>
                            </view>
                        </view>
                        <view class="{{styles['users-link__arrow']}}">
                            <image src="{{arrowRight}}" class="{{styles['users-link__arrow--active']}}"></image>
                        </view>
                    </view>
                </block>
                <block wx:else>
                    <block>
                        <block wx:if="{{env === 'swan'}}">
                            <view class="{{styles['users-link__item']}}">
                                <view class="{{styles['users-link__icon']}}">
                                    <image src="{{customer}}" class="{{styles['users-link__icon-help']}}"></image>
                                </view>
                                <view>
                                    <view class="{{styles['users-link__title']}}"><text class="{{styles['users-link__title-text']}}">在线客服 </text>
                                    </view>
                                    <view class="{{styles['users-link__subtitle']}}">
                                        <button size="mini" plain="{{true}}" open-type="contact">客服</button>
                                    </view>
                                </view>
                                <view class="{{styles['users-link__arrow']}}">
                                    <image src="{{arrowRight}}" class="{{styles['users-link__arrow--active']}}"></image>
                                </view>
                            </view>
                        </block>
                        <block wx:else>
                            <block wx:if="{{env === 'alipay'}}">
                                <view class="{{styles['users-link__item']}}">
                                    <view class="{{styles['users-link__icon']}}">
                                        <image src="{{customer}}" class="{{styles['users-link__icon-help']}}"></image>
                                    </view>
                                    <view bindtap="onShowContact" data-e-tap-so="this">
                                        <view class="{{styles['users-link__title']}}"><text class="{{styles['users-link__title-text']}}">客服电话</text>
                                        </view>
                                        <view class="{{styles['users-link__subtitle']}}"><text class="{{styles['users-link__subtitle-text']}}">400-803-4725</text>
                                        </view>
                                    </view>
                                    <view class="{{styles['users-link__arrow']}}">
                                        <image src="{{arrowRight}}" class="{{styles['users-link__arrow--active']}}"></image>
                                    </view>
                                </view>
                            </block>
                        </block>
                    </block>
                </block>
            </block>
            <block wx:if="{{env === 'qq'}}">
                <view class="{{styles['users-link__item']}}" bindtap="onShareAppMessage" data-e-tap-so="this">
                    <view class="{{styles['users-link__icon']}}">
                        <image src="{{praise}}" class="{{styles['users-link__icon-help']}}"></image>
                    </view>
                    <view class="{{styles['users-link__title']}}"><text class="{{styles['users-link__title-text']}}">分享给小伙伴</text>
                    </view>
                    <view class="{{styles['users-link__subbutton--qq']}}">
                        <button size="mini" type="default" open-type="share">去分享</button>
                    </view>
                    <view class="{{styles['users-link__arrow']}}">
                        <image src="{{arrowRight}}" class="{{styles['users-link__arrow--active']}}"></image>
                    </view>
                </view>
            </block>
        </view>
    </view>
</block>