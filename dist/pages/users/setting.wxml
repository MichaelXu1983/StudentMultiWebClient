<block wx:if="{{$taroCompReady}}">
    <view class="{{styles['setting']}}">
        <view class="{{styles['setting-link']}}">
            <navigator url="/pages/webview/about" class="{{styles['index__Navigator']}}">
                <view class="{{styles['setting-link__item']}}">
                    <view class="{{styles['setting-link__title']}}"><text class="{{styles['setting-link__title-text']}}">关于我们</text>
                    </view>
                    <view class="{{styles['setting-link__extra']}}"><text class="{{styles['setting-link__extra-text']}}">{{SDKVersion ? 'v' + SDKVersion : ''}}</text>
                    </view>
                    <view class="{{styles['setting-link__arrow']}}">
                        <image src="{{arrowRight}}" class="{{styles['setting-link__arrow--active']}}"></image>
                    </view>
                </view>
            </navigator>
            <navigator url="/pages/webview/agreement" class="{{styles['index__Navigator']}}">
                <view class="{{styles['setting-link__item']}}">
                    <view class="{{styles['setting-link__title']}}"><text class="{{styles['setting-link__title-text']}}">使用协议</text>
                    </view>
                    <view class="{{styles['setting-link__arrow']}}">
                        <image src="{{arrowRight}}" class="{{styles['setting-link__arrow--active']}}"></image>
                    </view>
                </view>
            </navigator>
            <navigator url="/pages/webview/privacy_detail" class="{{styles['index__Navigator']}}">
                <view class="{{styles['setting-link__item']}}">
                    <view class="{{styles['setting-link__title']}}"><text class="{{styles['setting-link__title-text']}}">隐私政策</text>
                    </view>
                    <view class="{{styles['setting-link__arrow']}}">
                        <image src="{{arrowRight}}" class="{{styles['setting-link__arrow--active']}}"></image>
                    </view>
                </view>
            </navigator>
        </view>
    </view>
</block>